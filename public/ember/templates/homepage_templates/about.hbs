{{#each model}}
    {{#with pageContent}}
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <h1 class="page-header">{{title}} <small>{{tagLine}}</small></h1>      
        </div>
      </div>

      <div class="row">
        <div class="col-md-6">
          <img class="img-responsive" {{bind-attr src="heroImg"}}>
        </div>
        <div class="col-md-6">
          <h2>{{welcome}}</h2>
          <p>{{description}}</p>
        </div>
      </div>

      <!-- Team Member Profiles -->

      <div class="row">
        <div class="col-lg-12">
          <h2 class="page-header">{{teamHeadline}}</h2>
        </div>
        {{#each member in team}}
        <div class="col-sm-4">
        	<img class="img-responsive" {{bind-attr src="img"}}>
            <h3>{{member.name}} <small>{{member.title}}</small></h3>
            <p>{{member.description}}</p>
        </div>
        {{/each}}
      </div>

      <!-- Our Customers -->

      <div class="row">
        <div class="col-lg-12">
          <h2 class="page-header">{{customersTitle}}</h2>
        </div>
        {{#each customer in customers}}
        <div class="col-md-2 col-sm-4 col-xs-6">    	
          <img class="img-responsive img-customer" {{bind-attr src="customer.img"}}>
          <p>{{customer.name}}</p>
        </div>
        {{/each}}
      </div>

    </div><!-- /.container -->
  {{/with}}
{{/each}}